/* pages/notes/notes.wxss */
.page {
  position: absolute;
  width: 100%;
  height: 100%;
}
.lists {
  left: 0;
  width: 100%;
  height: 90%;
}
.notes-list{
   position: relative;
    border-bottom: 2px solid #88AA00;
}
.note-id{
  overflow: hidden;
  height: 60px;
  padding: 20px;
}
.note-details{
font-size: 15px;
color: #212121;
line-height: 1;
}
.note-createtime{
  position: absolute;
  color: #212121;
  font-size: 14px;
  line-height: 1;
  bottom: 5%;
}
.delete{
  text-align: center;
  position: absolute;
  bottom: 20%;
  margin-left: 80%;
  border: 1px solid #88AA00;
}
.add {
  position: absolute;
  bottom:0;
  width: 40px;
  height: 40px;
  text-align: center;
  margin-left: 83%;
  border: 1px solid #88AA00;
  border-radius: 50%;
}
.add image{
  margin-top: 12%;
  width: 30px;
  height: 30px;
}